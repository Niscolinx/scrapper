{
  "manifest_version": 2,
  "name": "LinkedIn scrapper",
  "description": "Scrapping LinkedIn profile data and organization emails",
  "version": "0.0.0.1",
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "LinkedIn scrapper",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
     "matches": ["http://*.linkedin.com/*"],
      "css": [],
      "js": ["content.js"]
    }
  ]
}
