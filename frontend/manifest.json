{
  "manifest_version": 2,
  "name": "LinkedIn scrapper",
  "description": "Scrapping LinkedIn profile data and organization emails",
  "version": "0.0.0.1",
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "LinkedIn scrapper",
    "default_popup": "popup.html"
  },
   "permissions": [
     "https://crunchbase-crunchbase-v1.p.rapidapi.com/odm-organizations/"
   ],
   "externally_connectable": {
     "ids": ["*"]
   },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": [],
      "js": ["content.js"]
    }
  ]
}
